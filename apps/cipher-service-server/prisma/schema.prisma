datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CrackingResult {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  plaintext  Json?
  ciphertext Json?
  key        Json?
  rounds     Int?
}

model Message {
  id         String   @id @default(cuid())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  plaintext  Json?
  ciphertext Json?
  rounds     Int?
  key        Json?
}
